<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <SplitPane dividerPositions="0.85" prefHeight="720.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <AnchorPane minWidth="0.0" prefWidth="100.0">
                    <children>
                        <TableView fx:id="bookTable" layoutX="-8.0" layoutY="14.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                            <columns>
                                <TableColumn fx:id="titleClm" minWidth="30.0" text="Titolo" />
                                <TableColumn fx:id="authorsClm" minWidth="30.0" text="Autori" />
                                <TableColumn fx:id="publishmentYearClm" minWidth="30.0" text="Anno di pubblicazione" />
                                <TableColumn fx:id="bookIdClm" minWidth="30.0" text="ISBN" />
                                <TableColumn fx:id="availableCopiesClm" minWidth="30.0" text="Copie" />
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
                <fx:include fx:id="sideBar" source="SideBarView.fxml" />
            </items>
        </SplitPane>
    </children>
</AnchorPane>
